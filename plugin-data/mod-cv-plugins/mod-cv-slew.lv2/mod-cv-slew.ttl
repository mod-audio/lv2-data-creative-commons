@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-slew>
a lv2:Plugin, mod:ControlVoltagePlugin;

doap:name "Slew Rate Limiter";
doap:license "https://spdx.org/licenses/GPL-2.0-or-later.html";
doap:developer [
    foaf:name "Ward Slager, Bram Giesen";
    foaf:homepage <>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "MOD";
    foaf:homepage <http://bramgiesen.com>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 0;

rdfs:comment """
Plugin to limit the speed of the rise and fall times of a CV signal.

Features:
Plugin by MOD Devices
""";

lv2:port
[
    a lv2:InputPort, lv2:CVPort, mod:CVPort;
    lv2:index 0;
    lv2:minimum -5.0 ;
    lv2:maximum 5.0 ;
    lv2:symbol "CVIn";
    lv2:name "CV Input";
],
[
    a lv2:OutputPort, lv2:CVPort, mod:CVPort;
    lv2:index 1;
    lv2:minimum -5.0 ;
    lv2:maximum 5.0 ;
    lv2:symbol "CVout";
    lv2:name "CV Output";
]
,
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "RiseTime" ;
    lv2:name "Rise Time";
    lv2:default 0.1 ;
    lv2:minimum 0.1 ;
    lv2:maximum 1000.0 ;
    units:unit units:ms ;
    lv2:portProperty  mod:tapTempo , mod:tempoRelatedDynamicScalePoints;
]
,
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "FallTime" ;
    lv2:name "Fall Time";
    lv2:default 0.1 ;
    lv2:minimum 0.1 ;
    lv2:maximum 1000.0 ;
    units:unit units:ms ;
    lv2:portProperty  mod:tapTempo , mod:tempoRelatedDynamicScalePoints;
]
,
[
     a lv2:InputPort ,
     lv2:ControlPort ;
     lv2:index 4 ;
     lv2:symbol "BYPASS" ;
     lv2:name "BYPASS" ;
     lv2:default 1.0 ;
     lv2:minimum 0.0 ;
     lv2:maximum 1.0 ;
     lv2:designation lv2:enabled;
     lv2:portProperty lv2:toggled;
]
.
